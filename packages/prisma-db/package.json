{
  "name": "@ai-store/prisma-db",
  "version": "1.0.0",
  "description": "Prisma database client and schema for AI Store",
  "license": "ISC",
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist node_modules .turbo",
    "typecheck": "tsc --noEmit",
    "generate": "dotenv -e .env -- prisma generate",
    "migrate:dev": "dotenv -e .env -- prisma migrate dev",
    "migrate:deploy": "dotenv -e .env -- prisma migrate deploy",
    "migrate:reset": "dotenv -e .env -- prisma migrate reset",
    "migrate:status": "dotenv -e .env -- prisma migrate status",
    "seed": "dotenv -e .env -- tsx prisma/seed.ts",
    "studio": "dotenv -e .env -- prisma studio",
    "postinstall": "prisma generate"
  },
  "devDependencies": {
    "@types/node": "^24.10.4",
    "@types/pg": "^8.16.0",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "prisma": "^7.2.0",
    "rimraf": "^6.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "pg": "^8.16.3"
  }
}
